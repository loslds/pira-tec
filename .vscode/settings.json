{
    "editor.rulers": [80],
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.tslint": true,
      "source.fixAll.stylelint": true
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "files.associations": {
      "*.js": "javascriptreact",
      "*.jsx": "javascriptreact"
    }
  }
